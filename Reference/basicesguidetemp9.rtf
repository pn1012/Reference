\par }\pard \ql \li0\ri0\widctlpar\tx0\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\tx8640\tx9360\faauto\rin0\lin0\itap0 \shading10000\cfpat8\cbpat8 {\fs24\insrsid10181827 
\par }\pard\plain \s15\ql \li0\ri0\widctlpar\tx0\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\tx8640\tx9360\faauto\rin0\lin0\itap0 \shading10000\cfpat8\cbpat8 \fs24\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {
