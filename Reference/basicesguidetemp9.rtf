\par }\pard \ql \li0\ri0\widctlpar\faauto\rin0\lin0\itap0 {\b\fs24\insrsid10181827 SAS}{\fs24\insrsid10181827 :
\par }\pard\plain \s17\ql \li0\ri0\widctlpar\faauto\rin0\lin0\itap0 \fs20\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {\f2\insrsid10181827 DATA newdata;
\par }\pard\plain \s18\ql \li0\ri0\widctlpar\tx0\tx9360\faauto\rin0\lin0\itap0 \fs20\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {\f2\insrsid10181827    SET olddata;
\par }\pard\plain \ql \li0\ri0\widctlpar\faauto\rin0\lin0\itap0 \fs20\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {\f2\insrsid10181827    N = grp1n + grp2n;
\par    d_by_t = (t_value * N) / (sqrt(df) * sqrt(grp1n * grp2n));
\par RUN;}{\fs24\insrsid10181827 
\par 
\par }\pard \ql \li0\ri0\widctlpar\tx0\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\tx8640\tx9360\faauto\rin0\lin0\itap0 \shading10000\cfpat8\cbpat8 {\b\fs24\insrsid10181827 Note. }{\fs24\insrsid10181827 The sign of }{
