\par }\pard\plain \s18\ql \li0\ri0\widctlpar\tx0\tx9360\faauto\rin0\lin0\itap0 \fs20\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {\f2\insrsid10181827    SET olddata;
\par }\pard\plain \ql \li0\ri0\widctlpar\faauto\rin0\lin0\itap0 \fs20\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {\f2\insrsid10181827    pool_var = (grp1css + grp2css) / (grp1n + grp2n);
\par    pool_sd = sqrt(pool_var);
\par    Cohens_d = (grp1mean - grp2mean) / pool_sd;
\par RUN;
\par }\pard \ql \li0\ri0\widctlpar\tx0\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\tx8640\tx9360\faauto\rin0\lin0\itap0 \shading10000\cfpat8\cbpat8 {\f2\insrsid10181827 
