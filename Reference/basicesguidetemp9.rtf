\par }\pard \ql \li0\ri0\keep\keepn\widctlpar\tx0\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\tx8640\tx9360\faauto\rin0\lin0\itap0 \shading10000\cfpat8\cbpat8 {\b\fs24\insrsid10181827 II-B-2: }{\fs24\insrsid10181827 Cohen's}{
